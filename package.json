  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/actarian/b-here.git#beta-bhere"
  },
  "engines": {
    "node": "^17 || ^16 || ^15 || ^14 || ^13 || ^12 || ^11 || ^10 || ^9 || ^8"
  },
  "main": "index.js",
  "files": [
    "server/**/*.*",
    "dist/**/*.*",
    "!dist/css/lib/*",
    "!dist/js/lib/*"
  ],
  "scripts": {
    "cert:ca": "mkcert create-ca",
    "cert:cert": "mkcert create-cert",
    "cert": "run-s cert:*",
    "serve": "node index.js",
    "run": "gulp build --target vendors && gulp build && npm run serve",
    "build": "gulp build",
    "dist": "gulp build --target dist",
    "test": "node test/selenium-test.js",
    "watch": "gulp start",
    "start": "run-p watch serve"
  },
  "dependencies": {
    "agora-access-token": "2.0.4",
    "agora-rtc-sdk": "3.6.9",
    "agora-rtm-sdk": "1.4.4",
    "body-parser": "1.19.2",
    "connect-multiparty": "2.2.0",
    "events": "3.3.0",
    "express": "4.17.3",
    "express-session": "1.17.2",
    "gsap": "3.9.1",
    "hls.js": "0.14.12",
    "html2canvas": "1.4.1",
    "https": "1.0.0",
    "mv": "2.1.1",
    "qrious": "4.0.2",
    "router5": "8.0.1",
    "router5-plugin-browser": "8.0.1",
    "rxcomp": "1.0.0-beta.21",
    "rxcomp-form": "1.0.0-beta.18",
    "rxjs": "6.6.2",
    "three": "0.138.3"
  },
  "devDependencies": {
    "gulpfile-config": "1.0.0-alpha.18",
    "mkcert": "1.5.0",
    "npm-run-all": "4.1.5",
    "selenium-webdriver": "4.0.0-beta.1"
  }
}
